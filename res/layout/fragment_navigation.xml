<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:adjustViewBounds="true"
        android:background="#0000"
        android:scaleType="fitCenter"
        android:src="@drawable/bigmap1000" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView1"
        android:layout_alignRight="@+id/imageView1"
        android:layout_margin="12dp"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        android:src="@drawable/blueadd48"
        android:background="?android:attr/selectableItemBackground" />

    <ImageView
        android:id="@+id/rest1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="14dp"
        android:layout_toLeftOf="@+id/guest1"
        android:src="@drawable/benchpin36"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/washroom1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton3"
        android:layout_marginTop="62dp"
        android:layout_toRightOf="@+id/imageButton3"
        android:src="@drawable/washroompin36"
        android:visibility="invisible" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/guest1"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="24dp"
        android:layout_toRightOf="@+id/guest1"
        android:background="?android:attr/selectableItemBackground"
        android:visibility="invisible" 
        android:src="@drawable/attractpin36" />

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButton3"
        android:layout_alignTop="@+id/rest1"
        android:background="?android:attr/selectableItemBackground"
        android:clickable="false"
        android:src="@drawable/attractpin36"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/rest2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton2"
        android:layout_marginTop="28dp"
        android:layout_toLeftOf="@+id/rest1"
        android:src="@drawable/benchpin36"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/guest1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rest1"
        android:layout_marginRight="17dp"
        android:layout_toLeftOf="@+id/food1"
        android:src="@drawable/customerpin36"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/food2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageButton2"
        android:layout_toLeftOf="@+id/rest2"
        android:src="@drawable/foodpin36"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/washroom2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageButton3"
        android:layout_toLeftOf="@+id/guest1"
        android:src="@drawable/washroompin36"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/food1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/washroom2"
        android:layout_marginBottom="17dp"
        android:layout_marginRight="14dp"
        android:layout_toLeftOf="@+id/imageButton3"
        android:src="@drawable/foodpin36"
        android:visibility="invisible" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/rest2"
        android:layout_toLeftOf="@+id/imageButton1"
        android:background="?android:attr/selectableItemBackground"
        android:paddingLeft="25dp"
        android:paddingTop="40dp"
        android:visibility="invisible"
        android:src="@drawable/attractspeechbub250" />

</RelativeLayout>
